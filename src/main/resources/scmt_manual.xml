<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="scmt_manual" >
    <select id="getListManual" parameterType="map" resultType="com.binar.grab.service.procedure.impl.BarangProsedure">
        select resid,  resnama,  resstok,  resharga,  ressatuan from public.getBarang1(
        #{rqnama,mode=IN,jdbcType=NUMERIC})
    </select>

    <update id="updatebarangxml" parameterType="map" statementType="CALLABLE">
        {CALL public.updatebarang(#{rqharga,mode=IN},#{rqnama,mode=IN},#{rqsatuan,mode=IN},#{rqstok,mode=IN},#{resid,mode=IN})}
    </update>
</mapper>
